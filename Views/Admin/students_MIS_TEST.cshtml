@using AdminstratorModule.Models

@model IEnumerable<Students_MIS>
@{
    ViewBag.Title = "students_MIS_TEST";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<center>@Html.ActionLink("Populate From Swift To MIS", "AddNewConfig", "Admin", new { @class = "btn btn-secondary" })</center>
<br />
<center>@Html.ActionLink("Populate From MIS To Swift", "AddNewConfig", "Admin", new { @class = "btn b" })</center>

<br />
<center>

    <table class="table-hover table-bordered  table-striped body-content text-center">
        <tr>
            <th>@Html.DisplayNameFor(ww => ww.ED_STUD_ID)</th>
            <th>@Html.DisplayNameFor(ww => ww.STUD_CODE)</th>
            <th>@Html.DisplayNameFor(ww => ww.NATIONAL_NUMBER)</th>
            <th>@Html.DisplayNameFor(ww => ww.FULL_NAME_AR)</th>
            <th>@Html.DisplayNameFor(ww => ww.FULL_NAME_EN)</th>
         
