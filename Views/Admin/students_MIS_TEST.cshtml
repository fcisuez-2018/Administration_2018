                <td>
                    @Html.DisplayFor(ww => item.STUD_CODE)
                </td>
                <td>
                    @Html.DisplayFor(ww => item.NATIONAL_NUMBER)
                </td>
                <td>
                    @Html.DisplayFor(ww => item.FULL_NAME_AR)
                </td>
                <td>
                    @Html.DisplayFor(ww => item.FULL_NAME_EN)
                </td>
           
                <td>
